---
import AboutMe from "../components/AboutMe.astro"
import Experience from "../components/Experience.astro"
import Footer from "../components/Footer.astro"
import Home from "../components/Home.astro"
import Projects from "../components/Projects.astro"
import Layout from "../layouts/Layout.astro"
---

<Layout
  title="Adrian German - Desarrollador web FullStack con experiencia profesional en el desarrollo de aplicaciones web a medida."
  description="Con más de 1 año de experiencia, soy un desarrollador web con alta capacidad de cumplir los requisitos y tareas solicitadas, ademas de aportar activamente en el desarrollo y planteamiento del proyecto."
>
  <main
    class="px-10 md:px-0 scroll-m-20 w-full mx-auto container lg:max-w-4xl md:max-w-2xl"
  >
    <Home />
    <Projects />
    <Experience />
    <AboutMe />
    <Footer />
  </main>
</Layout>
